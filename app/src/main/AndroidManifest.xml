<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="gerenciador.engefourjunior.com.gerenciadorengefour">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CadastrarClienteActivity" />
        <activity android:name=".ConsultarClienteActivity" />
        <activity android:name=".EditarClienteActivity" />
        <activity android:name=".CadastrarProdutoActivity" />
        <activity android:name=".ConsultarProdutoActivity" />
        <activity android:name=".EditarProdutoActivity" />
        <activity android:name=".CadastrarVendaActivity" />
        <activity android:name=".ConsultarVendaActivity"/>
        <activity android:name=".EditarVendaActivity" />
    </application>

</manifest>